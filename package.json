{
  "name": "promopro-agents-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:app": "./start.sh",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && ./venv/bin/python -m uvicorn api:app --reload --port 8000",
    "dev": "cd frontend && npm run dev:full",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "lint": "cd frontend && npm run lint",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && source venv/bin/activate && pip install -r requirements.txt"
  },
